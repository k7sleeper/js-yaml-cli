{
  "name": "js-yaml-cli",
  "version": "0.2.0",
  "description": "Command-line utilities to convert YAML files to JSON files and vice versa.",
  "keywords": [
    "yaml",
    "json",
    "parser",
    "serializer",
    "cli"
  ],
  "homepage": "https://github.com/k7sleeper/js-yaml-cli",
  "author": {
    "name": "Klaus Sevensleeper",
    "email": "k7sleeper@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/k7sleeper/js-yaml-cli/issues"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/k7sleeper/js-yaml-cli/blob/master/LICENSE"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/k7sleeper/js-yaml-cli.git"
  },
  "main": "./lib",
  "bin": {
    "yaml2json": "bin/yaml2json.js",
    "json2yaml": "bin/json2yaml.js"
  },
  "scripts": {
    "build"     : "grunt build",
    "prepublish": "npm run build",
    "test"      : "jasmine-node --color --coffee --captureExceptions test"
  },
  "dependencies": {
    "argparse": "~ 0.1.15",
    "js-yaml": "~ 2.1.3",
    "lodash": "~2.2.1",
    "underscore.string": "~2.3.3",
    "readdirp": "~0.3.1"
  },
  "devDependencies": {
    "shelljs": "~0.2.6",
    "coffee-script": ">=1.6.0",
    "grunt": "~0.4.1",
    "grunt-contrib-copy": "~0.4.1",
    "grunt-contrib-coffee": "~0.7.0",
    "grunt-contrib-watch": "~0.5.3",
    "jasmine-node": "~1.11.0",
    "jasmine-expect": "~1.20.0"
  },
  "optionalDependencies": {},
  "engineStrict": true,
  "engines": {
    "node": ">= 0.10.21"
  },
  "preferGlobal": true
}
